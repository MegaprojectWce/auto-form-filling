
<div class="container mt-5">

  <h3>
    Submitted Responses</h3>
  <div class="table-responsive">
    <table class="table table-bordered table-hover" style="width: fit-content;">
      <thead>
        <tr>
          <th>Sr.no.</th>
          <th>View</th>
          <th>Name</th>
          <th>Gender</th>
          <th>Birth Date</th>
          <th>Fathers name</th>
          <th>Mothers name</th>
          <th>Do you belong to minority</th>
          <th>Email</th>
          <th>Mobile No.</th>
          <th>Nationality</th>
          <th>Marital Status</th>
          <th>Community</th>
          <th>Address</th>
          <th>Education</th>
          <th>Degree Subject in Graduation</th>
          <th>Percentage at graduation level</th>
          <th>Applied exams</th>
          <th>No. of attempts in Civil Services</th>
          <th>Language medium  for paper 1 to 5</th>
          <th>Language medium  paper 6 and 7</th>
          <th>Hailing from Arunachal Pradesh/Manipur/Meghalaya ?</th>
        </tr>
      </thead>
      <tbody>
        {{#each list}}
          <tr>
            <td>{{this.SerialNumber}}</td>
            <td> <a href="/{{this._id}}"><i class="fa fa-eye fa-lg" aria-hidden="true"></i></a></td>
            <td>{{this.name}}</td>
            <td>{{this.gender}}</td>
            <td>{{this.birthdate}}</td>
            <td>{{this.fathername}}</td>
            <td>{{this.mothername}}</td>
            <td>{{this.minority}}</td>
            <td>{{this.email}}</td>
            <td>{{this.nationality}}</td>
            <td>{{this.maritalstatus}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.community}}</td>
            <td>{{this.address}}</td>
            <td>{{this.education}}</td>
            <td>{{this.degreesubjectingraduation}}</td>
            <td>{{this.graduationpercentage}}</td>
            <td>{{this.examinationyouliketobeconsidered}}</td>
            <td>{{this.numberofattempts}}</td>
            <td>{{this.languagemediumforexaminationforpaper1topaper5}}</td>
            <td>{{this.languagemediumforexaminationforpaper6andpaper7}}</td>
            <td>{{this.doyouhailfromArunachalPradeshManipur}}</td>
            <td>
              <a
                href="/delete/{{this._id}}"
                onclick="return confirm('Are you sure to delete this record ?');"
              ><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
